        '404':
          description: Incorrect data provided 
          
  /auth/login:
    get:
      tags:
      - auth
      summary: Login by user
      description: Login by user
      operationId: logUser
      parameters:
        - name: login
          in: query
          description: The user name for login
          required: true
          schema:
            type: string
        - name: password
          in: query
          description: The password for login in clear text
          required: true
          schema:
            type: string
        - name: rememberMe
          in: query
          required: false
          schema:
            type: boolean
      responses:
        '200':
          description: successful operation
          headers:
            X-Rate-Limit:
              description: calls per hour allowed by the user
              schema:
                type: integer
                format: int32
            X-Expires-After:
              description: date in UTC when token expires
              schema:
                type: string
                format: date-time
          content:
            application/xml:
              schema:
                type: string
            application/json:
              schema:
                type: string
        '400':
          description: Invalid username/password supplied
                
  /auth/passwordreset:
    post:
      tags:
        - auth
      summary: Reset password
      description: Reset the password for the user
      operationId: resetPass
      responses:
        '200':
          description: Success  
      security:
      - valuedIn_auth:
          - write:users  
          - read:users
          
  /auth/passwordchange:
    post:
