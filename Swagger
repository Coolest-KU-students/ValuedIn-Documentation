  /chats:
    get:
      tags:
        - chats
      summary: List all Chats
      description: This can only be done by the logged in user.
      operationId: listChats
      responses:
        '200':
          description: Success
          content:
            application/json:
              schema:
                $ref: '#/components/schemas/ChatInfoPage'
            application/xml:
              schema:
                $ref: '#/components/schemas/ChatInfoPage'
